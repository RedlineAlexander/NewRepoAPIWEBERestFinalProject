@model Orders

    <form asp-controller="Orders" asp-action="Create" method="post">
        <div asp-validation-summary="All"></div>
        <label asp-for="BookingDate"></label><br>
        <input asp-for="BookingDate" type="datetime-local" /><br>

        <label asp-for="BookingPaymentStatusRef"></label><br>
