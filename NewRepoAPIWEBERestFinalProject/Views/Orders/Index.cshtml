@model List<Orders>

<table class="table" style="text-align: center">
    <tr>
        <th>BookingId</th>
        <th>ServiceID</th>
        <th>PatientID</th>
        <th>BookingDate</th>
        <th>BookingPaymentStatusRef</th>
    </tr>
    @foreach (var orders in Model)
    {
<tr>
    <td>@orders.BookingId</td>
    <td>@orders.BookingDate</td>
    <td>@orders.BookingPaymentStatusRef</td>
</tr>
    }
</table>



@{
}
