@model List<Services>
    <table class="table" style="text-align: center">
        <tr>
            <th>ServiceId</th>
            <th>ServiceNameLanguage</th>
            <th>ServiceCode</th>
            <th>ServicePriceRate</th>
            
        </tr>
        @foreach (var services in Model)
        {
            <tr>
                <td>@services.ServiceID</td>
                <td>@services.ServiceNameLanguage</td>
                <td>@services.ServiceCode</td>
                <td>@services.ServicePriceRate</td>
              
            </tr>
        }
    </table>


@{
}
